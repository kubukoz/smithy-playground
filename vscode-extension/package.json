{
  "name": "smithy-playground",
  "displayName": "Smithy Playground",
  "description": "Experimental integration for queries in the Smithy language.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.1"
  },
  "publisher": "kubukoz",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubukoz/smithy-playground"
  },
  "homepage": "https://github.com/kubukoz/smithy-playground",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:*.smithyql"
  ],
  "main": "./out/extension",
  "contributes": {
    "notebooks": [
      {
        "type": "smithyql",
        "displayName": "SmithyQL",
        "selector": [
          {
            "filenamePattern": "*.smithyql"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "@types/vscode": "1.63.1"
  },
  "devDependencies": {
    "vsce": "^2.5.3"
  }
}
